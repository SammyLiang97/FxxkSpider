{
  "name": "fxxkSpider",
  "version": "0.8.0",
  "main": "index.js",
  "repository": "https://github.com/Lurance/FxxkSpider.git",
  "author": "Sammy",
  "scripts": {
    "start:http": "cross-env NODE_ENV=development nodemon src/index.ts",
    "start:page": "cd ./src/page && npm run start",
    "build:page": "cd ./src/page && npm run build",
    "run:restore": "ts-node scripts/restore_subscribers.ts",
    "run:fetch_name_id": "ts-node scripts/fetch_goods_nameid.ts",
    "run:fetch_steam_price": "ts-node scripts/fetch_goods_steam_price.ts",
    "run:task_flush": "ts-node scripts/task_flush.ts",
    "run:schedule_jobs": "ts-node jobs/main.ts",
    "fetch_name_id": "pm2 start --name=\"name_id\" npm -- run run:fetch_name_id",
    "fetch_steam_price": "pm2 start --name=\"steam_price\" npm -- run run:fetch_steam_price",
    "task_flush": "pm2 start --name=\"flush\" npm -- run run:task_flush",
    "restore": "pm2 start --name=\"restore\" npm -- run run:restore",
    "schedule_jobs": "pm2 start --name=\"schedule\" npm -- run run:schedule_jobs",
    "start": "pm2 start --name=\"web\" ts-node -- src/index.ts"
  },
  "license": "MIT",
  "private": true,
  "nodemonConfig": {
    "delay": "2500",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.10",
    "@types/core-decorators": "^0.20.0",
    "@types/js-md5": "^0.4.2",
    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/koa-router": "^7.0.31",
    "@types/koa-session": "^5.7.4",
    "@types/koa-static": "^4.0.0",
    "@types/mathjs": "^4.4.1",
    "@types/mongoose": "^5.3.7",
    "@types/ms": "^0.7.30",
    "@types/node": "^10.11.0",
    "@types/node-schedule": "^1.2.2",
    "@types/request": "^2.48.1",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.4",
    "pm2": "^3.2.8",
    "ts-loader": "^5.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "yarn": "^1.22.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "better-xlsx": "^0.7.0",
    "cheerio": "^1.0.0-rc.2",
    "core-decorators": "^0.20.0",
    "js-md5": "^0.7.3",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-session": "^5.10.1",
    "koa-static": "^5.0.0",
    "mathjs": "^5.1.2",
    "moment": "^2.22.2",
    "mongoose": "^5.7.5",
    "ms": "^2.1.1",
    "node-schedule": "^1.3.1",
    "object-assign": "^4.1.1",
    "promise": "^8.0.2",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "routing-controllers": "^0.7.7",
    "typeorm": "^0.2.7",
    "whatwg-fetch": "^3.0.0"
  }
}
